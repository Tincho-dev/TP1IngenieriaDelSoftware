@model IEnumerable<TP1IngenieriaDelSoftware.Models.Modelo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SKU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Denominacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lim_inferiorO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lim_supO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lim_inferiorR)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lim_supR)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SKU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Denominacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lim_inferiorO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lim_supO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lim_inferiorR)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lim_supR)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
